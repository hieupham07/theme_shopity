		position: relative; text-align: center;
		img{object-fit: cover;min-height:150px;object-position: center center;}
		.headtitle-block{		
			position: absolute; top: 50%; left: 0; right: 0;
			-webkit-transform: translateY(-50%);transform: translateY(-50%);
			h1{
				font-size: 30px;color: {{settings.about01_banner_txtcolor}};font-weight: 500;letter-spacing: 1px;
				margin: 15px 0;	padding:0 ;text-align: center;text-transform: uppercase;
			}
		}
		@media(max-width:767px){
			.headtitle-block h1{font-size:22px;margin: 10px 0;}		
		}
	}
	.breadcrumb-shop{
		background: transparent;
		.breadcrumb-arrows{
			-webkit-justify-content: center;justify-content: center;padding: 0;
		}
	}
}
.wrapper-page-content{
	.wrapbox-content-page{
		padding-bottom: 30px;
		ul{list-style-type: initial;padding-left: 40px;margin-bottom: 15px;}
	}
	.banner-image-page{
		a{
			display: block;overflow: hidden;
			img{
				-webkit-transform: scale(1);
				-ms-transform: scale(1);
				transform: scale(1);
				-webkit-transition: -webkit-transform .4s ease-out;
				transition: -webkit-transform .4s ease-out;
				-o-transition: transform .4s ease-out;
				transition: transform .4s ease-out;
				transition: transform .4s ease-out,-webkit-transform .4s ease-out;
			}
			&:hover{
				img{
					-webkit-transform: scale(1.05);
					-ms-transform: scale(1.05);
					transform: scale(1.05);
				}
			}
		}
	}
}
.layout-pageContact{
	.heading-page-contact{
		h2{
			font-size: 25px;
			&:after{
				content: '';
				display: block;
				width: 32px;
				height: 2px;
				background: #252a2b;
				margin: 15px 0;
			}
		}
	}
	.description-contact{
		p{
			margin-bottom: 15px;
			font-weight: 400;
			text-transform: none;
			font-size: 15px;
			letter-spacing: 0;
		}
	}
	.wrapper-info-contact{
		span{
			text-transform: uppercase;
			font-size: 15px;
			color: #333;
			font-weight: 500;
		}
		.info-contact_left{width: 35%;}
		.info-contact_right{
			width: calc(100% - 35%);
			p{font-weight: 600;color: $colorFullPage;}
		}
		.list-info-contact li{margin-bottom: 15px;}
	}
}
.banner-contact-header{
	position: relative;
	.banner-contact-header_title{
		position: absolute;
		top: 50%;
		left: 0;
		right: 0;
		-webkit-transform: translateY(-50%);
		transform: translateY(-50%);
		h1{
			font-size: 30px;
			color: {{settings.banner_contact_txtcolor}};
			letter-spacing: 1px;
			margin: 20px 0;
			text-align: center;
			text-transform: uppercase;
			@media(max-width: 767px){
				&{font-size: 22px;}
			}
		}
	}
}
.layout-searchPage{
	@media(min-width: 992px){
		.search-list-results{
			.product-loop{
				-ms-flex: 0 0 20%;
				flex: 0 0 20%;
				max-width: 20%;
			}
		}
	}
}
.wrapper-collection-1, .wrapper-collection-3{
	.content-product-list{
		@media(min-width: 992px){
			.product-loop{
				-ms-flex: 0 0 20%;
				flex: 0 0 20%;
				max-width: 20%;
			}
		}
	}
}
/*--------------------------------------------------------------------------------
8. PAGE CART
--------------------------------------------------------------------------------*/
.layoutPage-cart{
	.heading-page-cart{
		h1{
			font-size: 25px;font-weight: 600;
			&:after{
				content: "";
				display: block;
				margin: 20px auto;
				width: 50px;
				height: 3px;
				background: #5c5c5c;
			}
		}
	}
}
.wrapbox-content-cart{
	.expanded-message{font-size: 18px;}
	.line-item{width: 100%;padding: 15px 0;}
	.line-item-container{border-bottom: 1px solid #dee2e6;}
	.title-count-cart{
		font-size: 16px;
		background: #f3f5f6;
		padding: 10px 15px;
		span{font-weight: 500;}
	}
	.line-item-image_wrapper{max-width: 100px;}
	.line-item-product_body{position: relative;}
	.line-item{
		&_title{
			h3{font-size: 16px;font-weight: 500;}
		}
		&_quantity{
			.qty-btn{
				width: 25px;
				height: 25px;
				font-size: 14px;
				font-weight: 500;
				outline: none;
				cursor: pointer;
				background: #fff;
				color: rgb(153, 153, 153);
				-webkit-appearance: none;
			}
			.item-quantity{
				width: 35px;
				height: 25px;
				font-size: 14px;
				-webkit-appearance: none;
				-moz-appearance: none;
				appearance: none;
			}
		}
		&_price{
			span strong{font-weight: 500;}
			del{font-size: 12px;}
		}
		&_remove-item-cart{
			position: absolute;
			top: 0;
			right: 0;
			font-size: 18px;
			color: #000;
		}
	}
	.reponsive-cart-noted{
		background: #f3f5f6;
		padding: 10px 15px;
		.checkout-note{
			.note-label{font-size: 15px;font-weight: 500;}
			.form-control{background: #fff;}
		}
	}
	@media(max-width: 767px){
		.line-item_title h3{font-size: 14px;}
		.line-item-image_wrapper{max-width: 70px;}
	}
}
.policy_return{
	h3{font-size: 15px;color: $colorFullPage;line-height: 1.5;font-weight: 500;}
	.list-group{
		>li{
			position: relative;
			line-height: 25px;
			&:before{
				font-family: "FontAwesome" !important;
				font-style: normal;
				font-weight: normal;
				text-decoration: inherit;
				content: "\f178";
				font-size: 10px;
				position: absolute;
				left: 0;
				opacity: 1;
			}
		}
	}
}
.wrap-order-summary{
	.order-title{font-size: 20px;}
	.order-total{
		font-size: 16px;
		span{color:#d29f13;font-size: 24px;font-weight: 500;}
	}
	.order-short-description{
		p{font-size: 13px;}
	}
	.order-action-checkout{
		a{
			font-size: 15px;
			background: #d29f13;
			padding: 10px 0;
			&:hover{background:#fff;color:#453536 !important; border:1px solid #d29f13;}
		}
	}
	.countine_order_cart{
		font-size: 16px;
		color: $colorTitle;
	}
	@media (min-width: 768px) and (max-width: 991px) {
		.order-title{font-size: 16px;}
		.order-total{
			span{font-size: 16px;}
		}
	}
}
/*--------------------------------------------------------------------------------
9. COLLECTION
--------------------------------------------------------------------------------*/
.collection-heading-page{
	.title{font-size: 30px;}
}
.collection-sortby{
	position: relative;
	&-title{
		.title-sort{
			display: inline-block;
			text-transform: uppercase;
			color: $colorFullPage;
			font-size: 13px;
			font-weight: 500;
			cursor: pointer;
			padding-right: 5px;
		}
		.sort-by-icon{
			height: 30px;display: block;
			svg{width: 30px;height: 30px;fill: $colorFullPage;transition: transform .3s ease-in;}
		}	
	}
	&-custom_option{
		.custom-select-menu{
			>li{
				position: relative;
				padding: 4px 25px 4px 15px;
				cursor: pointer;
				span{display: block;color: $colorFullPage;}
				&:hover{
					background: #f3f5f6;
					color: #000;
				}
				&:before{
					content: "";
					position: absolute;
					right: 10px;
					top: 9px;
					margin: auto;
					z-index: 1;
					width: 12px;
					height: 6px;
					border: 1.5px solid $colorFullPage;
					border-top-style: none;
					border-right-style: none;
					-webkit-transition: -webkit-transform 0.4s cubic-bezier(0.45, 1.8, 0.5, 0.75);
					-moz-transition: -moz-transform 0.4s cubic-bezier(0.45, 1.8, 0.5, 0.75);
					transition: transform 0.4s cubic-bezier(0.45, 1.8, 0.5, 0.75);
					-webkit-transform: rotate(-45deg) scale(0, 0);
					-moz-transform: rotate(-45deg) scale(0, 0);
					transform: rotate(-45deg) scale(0, 0);
				}
				&.active{
					font-weight: 600;
					&:before{
						-webkit-transform: rotate(-45deg) scale(1, 1);
						-moz-transform: rotate(-45deg) scale(1, 1);
						transform: rotate(-45deg) scale(1, 1);
					}
				}
			}
		}
		.collection-sortby-close_mb{display: none;}
	}
	@media(min-width: 992px){
		&-custom_option{
			position: absolute;
			top: 130%;
			right: 0;
			width: 200px;
			background: #fff;
			padding: 10px 0 10px;
			opacity: 0;
			visibility: hidden;
			transition: all .3s ease-in;
			z-index: 99;
			-webkit-box-shadow: 0 6px 12px rgba(0,0,0,0.175);
			box-shadow: 0 6px 12px rgba(0,0,0,0.175);
		}
		&.open-sort{
			.collection-sortby-custom_option{
				opacity: 1;visibility: visible;top: 100%;
			}
		}
	}
	@media(max-width: 991px){
		&-custom_option{
			.custom-select-menu{
				padding: 10px 0 10px 0;
				> li {margin-bottom: 5px;padding-right: 35px;}
			}
		}
	}
}
.collection-options{
	&-filter{
		span.options-filter-title{
			display: inline-block;text-overflow: ellipsis;white-space: nowrap;max-width: 100px;overflow: hidden;
			padding-right: 5px;font-size: 13px;color: $colorFullPage;text-transform: uppercase;font-weight: 500;
		}
		.options-filter-box-icon{
			position: relative;
			svg{width: 30px;height: 30px;fill: $colorFullPage;display: block;}
			.icon-filter-close{display: none;}
		}
		&.filters-open{
			.options-filter-box-icon{
				.icon-filter-collections{display: none;}
				.icon-filter-close{display: block;}
			}
		}
	}
}
.sidebar-filter-close{display: none;}
