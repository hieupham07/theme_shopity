.select-swatch{
	.swatch{
		width: 100%;
		float: left;
		input{display: none;}
		.header-swatch{
			font-size: 14px;
			font-weight: bold;
			color: black;
			display: block;
			span{
				font-size: 12px;
				display: inline-block;
				font-weight: 500;
				color: #3966b8;
				font-size: 90%;
				margin-left: 3px;
			}
		}
		.select-swap{
			display: inline-block;
			vertical-align: middle;
			padding: 12px 0;
		}
		.swatch-element{
			position: relative;
			display: inline-block;
			margin-right: 12px;
			label{
				position: relative;
				cursor: pointer;
				float: left;
				margin: 0;
				color: #000;
				font-size: 13px;
				font-weight: 600;
				text-align: center;
				line-height: 30px;
				white-space: nowrap;
				text-transform: uppercase;
			}
			&.color{
				label{
					padding: 0;
					overflow: hidden;
					width: 25px;
					height: 25px;
					border-radius: 100%;
					box-shadow: 0px 0px 0px 1px #dee2e6;
					-webkit-box-shadow: 0px 0px 0px 1px #dee2e6;
					-moz-box-shadow: 0px 0px 0px 1px #dee2e6;
					-o-box-shadow: 0px 0px 0px 1px #dee2e6;
					&.sd{
						-webkit-box-shadow: 0px 0px 0px 1px #d3d3d3, 0 0 0 4px #ffffff, 0 0 0 5px #333e48;
						-moz-box-shadow: 0px 0px 0px 1px #d3d3d3, 0 0 0 4px #ffffff, 0 0 0 5px #333e48;
						-o-box-shadow: 0px 0px 0px 1px #d3d3d3, 0 0 0 4px #ffffff, 0 0 0 5px #333e48;
						box-shadow: 0px 0px 0px 1px #d3d3d3, 0 0 0 4px #ffffff, 0 0 0 5px #333e48;
					}
				}
				span{
					display: block;
					width: 100%;
					height: 100%;
					margin-top: 0;
					font-size: 0;
				}
			}
			&:not(.color){
				&.soldout{
					label{
						opacity: 0.4;
						filter: alpha(opacity=40);
						pointer-events: none; 
					}
				}
				label{
					&:before{
						width: 17px;
						height: 17px;
						border: 1px solid #ccc;
						display: inline-block;
						margin-right: 5px;
						content: "";
						border-radius: 50%;
						margin-bottom: -4px;
					}
					&:after{
						content: "\f00c";
						font-family: 'FontAwesome'!important;
						position: absolute;
						top: 0;
						left: 3px;
						font-size: 10px;
						color: #fff;
						display: none;
						z-index: 2;
						-webkit-transition: all 0.45s ease;
						-o-transition: all 0.45s ease;
						transition: all 0.45s ease;
					}
					&.sd{
						&:before{
							background: #000;
							border-color: #000;
						}
						&:after{
							display: block;
						}
					}
				}
			}
			&:hover{
				.tooltip{
					visibility: visible;
					opacity: 1;
					filter: alpha(opacity=100);
					-webkit-transform: translateY(0px);
					-moz-transform: translateY(0px);
					-ms-transform: translateY(0px);
					-o-transform: translateY(0px);
				}
			}

		}
		.tooltip{
			position: absolute;left: 50%;bottom: 100%;display: block;
			min-width: 60px;margin-left: -30px;margin-bottom: 15px;
			padding: 4px 8px;color: #fff;background: #000;
			font-size: 11px;text-align: center;visibility: hidden;
			opacity: 0;filter: alpha(opacity=0);-webkit-transform: translateY(10px);
			-moz-transform: translateY(10px);-ms-transform: translateY(10px);
			-o-transform: translateY(10px);-webkit-transition: all 0.25s ease-out;
			-o-transition: all 0.25s ease-out;transition: all 0.25s ease-out;
			-webkit-box-shadow: 2px 2px 6px rgba(0,0,0,0.28);box-shadow: 2px 2px 6px rgba(0,0,0,0.28);
			z-index: inherit;
			&:before{
				content: "";
				position: absolute;
				left: 0;
				bottom: -20px;
				display: block;
				width: 100%;
				height: 20px;
			}
			&:after{
				content: "";
				position: absolute;
				left: 50%;
				bottom: -5px;
				width: 0;
				height: 0;
				margin-left: -5px;
				border-left: 5px solid transparent;
				border-right: 5px solid transparent;
				border-top: 5px solid #000;
			}
		}
	}
}

