	@media(min-width: 992px){
		.product-gallery{
			width: 50%;
			float: left;
			margin-bottom: 10px;
			&:nth-of-type(2n + 1){padding-right: 5px;}
			&:nth-of-type(2n){padding-left: 5px;}
			a{border: 1px solid #dee2e6;}
		}
	}
}
.productDetail_style_03{
	.productGallery_thumb{width: 90px;padding-right: 15px;}
	.productGallery_slider{width: calc(100% - 90px);}
	.product-list-lazy{
		opacity: 0;
		transition: opacity .5s ease-in;
		-webkit-transition: opacity .5s ease-in;
		&.slick-initialized{opacity: 1;}
	}
	.productList-slider{
		button.slick-arrow{
			position: absolute;
			top: 50%;
			width: 35px;
			height: 35px;
			line-height: 35px;
			font-size: 0;
			border: 0;
			cursor: pointer;
			z-index: 10;
			background-color: transparent;
			-webkit-transform: translateY(-50%);
			-moz-transform: translateY(-50%);
			transform: translateY(-50%);	
			&.slick-prev {
				left: 0;
				&.slick-disabled{opacity: 0;visibility: hidden;}
				&:before{content: "\f104";}
			}
			&.slick-next{
				right: 0;
				&.slick-disabled{opacity: 0;visibility: hidden;}
				&:before{content: "\f105";}
			}
			&:before{content: "";font-family: "FontAwesome";font-size: 35px;}
		}
		ul.slick-dots{
			position: relative;text-align: center;margin: 10px 0;
			li{
				display: inline-block;
				line-height: 17px;
				button{
					border-radius: 50%;
					width: 10px;
					height: 10px;
					margin: 5px;
					border-width: 2px;
					background: #dbd9d9;
					border: 1px solid #fff;
					font-size: 0;
					outline: none;
					cursor: pointer;
					-webkit-transform: scale(1);
					transform: scale(1);
					-webkit-transition: color 0.2s ease-in-out, background 0.2s ease-in-out, transform 0.2s ease-in-out;
					-moz-transition: color 0.2s ease-in-out, background 0.2s ease-in-out, transform 0.2s ease-in-out;
					transition: color 0.2s ease-in-out, background 0.2s ease-in-out, transform 0.2s ease-in-out;
				}
				&.slick-active{
					button{
						background: #333;
						transform: scale(1.3);
					}
				}
			}
		}
	}
	.product-thumb{
		.product-thumb-item{
			display: block;
			border: 1px solid #f3f3f3;
			margin-bottom: 8px;
			&:hover{
				border-color: $colorMain;
			}
		}
		&.slick-current{
			.product-thumb-item{
				border-color: $colorMain;
			}
		}
	}
	@media (min-width: 768px) and (max-width: 991px) {
		.productGallery_thumb{width: 110px;}
		.productGallery_slider{width: calc(100% - 110px);}
	}
	@media(max-width: 767px){
		.productGallery_thumb{display: none;}
		.productGallery_slider{width: 100%;}
	}
}
.productDetail-related{
	.productRelated-title{
		h2{font-size: 28px;margin: 0 0 25px;}
	}
	.productRelated-content{
		.content-product-list{
			.product-loop{
				display: inline-block;opacity: 0;transition: opacity .5s ease;
			}
			&.owl-loaded{
				.product-loop{
					display: block;opacity: 1;
				}
			}
		}
	}
}
.productDetail-recently-viewed {
	.productViewed-title{
		h2{font-size: 28px;margin: 0 0 25px;}
	}
	.productViewed-content_list{
		.product-loop{
			display: inline-block;opacity: 0;transition: opacity .5s ease;
		}
		&.owl-loaded{
			.product-loop{
				display: block;opacity: 1;
			}
		}
	}
	@media(max-width: 767px){
		&{border-top: 1px solid #dee2e6;margin-top: 24px;}
	}
}
.productDetail-information{
	.sliderOwl-product{
		.owl-nav{
			button{
				position: absolute;
				top: 50%;
				-webkit-transform: translateY(-50%);
				-moz-transform: translateY(-50%);
				-ms-transform: translateY(-50%);
				-o-transform: translateY(-50%);
				transform: translateY(-50%);
				padding: 0;
				margin: 0px;
				font-size: 0;
				height: 35px;
				width: 35px;
				line-height: 35px;
				outline: none;
				text-align: center;
				display: inline-block;
				background: transparent;
				&.owl-prev{
					left: 0;
					&.disabled{opacity: 0;}
					&:before{
						content: "\f104";
						font-family: "FontAwesome";
						font-size: 33px;
						display: inline-block;
						color: #000;
					}
				}
				&.owl-next{
					right: 0;
					&.disabled{opacity: 0;}
					&:before{
						content: "\f105";
						font-family: "FontAwesome";
						font-size: 33px;
						display: inline-block;
						color: #000;
					}
				}
			}
		}
		.owl-dots{
			position: relative;text-align: center;margin: 10px 0;
			.owl-dot{
				display: inline-block;
				width: 8px;
				height: 8px;
				margin: 5px;
				-webkit-border-radius: 20px;
				-moz-border-radius: 20px;
				border-radius: 20px;
				background: #dbd9d9;
				transform: scale(1);
				transition: transform 0.2s ease-in;
				&.active{background: #333;transform: scale(1.3);}
			}
		}
	}
	@media(max-width: 991px){
		.owl-carousel{
			opacity: 0;
			visibility: hidden;
			transition: opacity 1s ease;
			-webkit-transition: opacity 1s ease;
			&.owl-loaded{opacity: 1;visibility: visible;}
		}
	}
}
#owlProductRelated,
#listViewed,
.noritake-conetent{
	button{
		position: absolute;
		top: 40%;
		width: 35px;
		height: 35px;
		line-height: 35px;
		border-radius: 50%;
		background-color: transparent;
		font-size: 0;
		outline: none;
		background-repeat: no-repeat;
		background-size: 16px;
		background-position: center;
		-webkit-transition: all 0.5s;
		-moz-transition: all 0.5s;
		transition: all 0.5s;
		&.owl-prev{
			left: 0;
			background-image: url(//theme.hstatic.net/1000409858/1000615297/14/arrow-left.png?v=67);
			&.disabled{display: none;}
		}
		&.owl-next{
			right: 0;
			background-image: url(//theme.hstatic.net/1000409858/1000615297/14/arrow-right.png?v=67);
			&.disabled{display: none;}
		}
	}
	@media(min-width: 768px){
		button{
			&.owl-prev,&.owl-next{
				opacity: 0;
				background-color: #fff;
				background-size: 10px;
				-webkit-box-shadow: 0 1px 5px rgba(54, 54, 54, 0.15);
				box-shadow: 0 1px 5px rgba(54, 54, 54, 0.15);
			}
			&.owl-prev{left: 60px;}
			&.owl-next{right: 60px;}
		}
		&:hover{
			button{
				&.owl-prev,&.owl-next{opacity: 1;}
				&.owl-prev{left: 15px;}
				&.owl-next{right: 15px;}
			}
		}
	}
	@media(max-width: 767px){
		&{margin-right: -15px;margin-left: -15px;}
		button{
			&.owl-prev.disabled + .owl-next{
				animation: marginLeft 1.2s ease-in-out infinite;
				animation-direction: normal;
			}
		}
	}
}
