/*------------------- Menu Desktop ---------------------*/
.scroll-body-up{
	.f-nav{
		.f-nav-one{
			> li.onMega {
				.f-nav-two{
					top: 73.2px !important;
				}
			}
		}
	}
}
.f-nav{
	.f-nav-one{
		> li{
			display: inline-block;
			vertical-align: middle;
			margin: 0 10px;
			> a{
				position: relative;
				display: block;
				font-size: 15px;
				text-transform: uppercase;
				font-weight: 400;
				cursor: pointer;
				color: #333;
				padding: 27px 0;
				> svg{
					width: 12px;
					height: 8px;
					margin-left: 2px;
					-webkit-transition: -webkit-transform .3s ease-in-out;
					-moz-transition: -moz-transform .3s ease-in-out;
					transition: transform .3s ease-in-out;
				}
				&:after{
					content: '';
					position: absolute;
					display: inline-block;
					width: 0;
					height: 3px;
					left: 0;
					bottom: -2px;
					background-color:  $colorHover;
					-webkit-transition: width .3s ease-in-out;
					-o-transition: width .3s ease-in-out;
					transition: width .3s ease-in-out;
				}
				@media(min-width: 992px) and (max-width: 1199px){
					&{font-size: 12px;}
				}
			}
			&:hover{
				> a{
					color: $colorHover;
					&:after{width: calc(100% - 2px);}
					svg{
						-webkit-transform: rotate(180deg);
						-moz-transform: rotate(180deg);
						transform: rotate(180deg);
					}
				} 
				> ul{
					opacity: 1 !important;
					visibility: visible !important;
					margin-top: 0 !important;
				}
			}
			&.onMega{
				&.onMega-2{
					.f-nav-two{
						.flex-all{
							&.flex-1, &.flex-2, &.flex-3, &.flex-4{ 
								width: 25%;
								li{
									width: 100%;
								}
							}
						}
					}
				}
				.f-nav-two{
					position: absolute;
					top: 100%;
					width: 100%;
					left:0;
					right: 0;
					background: #fff;
					min-width: 140px;
					margin-top: 30px;
					border-radius: 0 0 3px 3px;
					box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
					//white-space: nowrap;
					visibility: hidden;
					opacity: 0;
					z-index: 1;
					transition: all 0.3s ease-in-out;
					display: flex;
					@media(min-width:1200px){
						padding:10px 60px 10px 70px;
					}
					.flex-all{
						display: flex;
						align-content: space-between;  
						flex-wrap: wrap; 
						&.flex-1{ 
							width: 20%;
						}
						&.flex-2{
							width: 40%;
							> li{
								width: 50%;
							}
						}
						&.flex-3{
							width: 20%;
						}
						&.flex-4{
							width: 20%;
						}
						> li > a{
							font-weight: 600;
							color: {{settings.header_02_color_text}};
						}
					}

					li{
						position: relative;
						a{
							padding: 9px 15px 9px 15px;
							text-align: left;
							display: block;
							font-size: 14px;
							text-transform: capitalize;
							font-weight: 300;
							svg{
								display: none;
							}
							img{
								max-width: 100%;
								height: auto;
							}
						}
					}
				}
			}
			&:not(.onMega){
				position: relative;
				.f-nav-two{
					position: absolute;
					top: 100%;
					background: #fff;
					min-width: 140px;
					margin-top: 30px;
					border-radius: 0 0 3px 3px;
					box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
					white-space: nowrap;
					visibility: hidden;
					opacity: 0;
					z-index: 1;
					transition: all 0.3s ease-in-out;
					li{
						position: relative;
						a{
							padding: 9px 25px 9px 15px;
							white-space: nowrap;
							text-align: left;
							display: block;
							font-size: 14px;
							text-transform: capitalize;
							font-weight: 300;
							svg{
								position: absolute;
								top: 12%;
								right: 7px;
								width: 15px;
								fill: $colorFullPage;
								-webkit-transform: translateY(50%);
								-moz-transform: translateY(50%);
								transform: translateY(50%);
								-webkit-transition: right .5s ease;
								-moz-transition: right .5s ease;
								transition: right .5s ease;
							}
						}
						&:hover{
							a {
								svg{
									right: 0;
								}
							}
							> ul{
								opacity: 1 !important;
								visibility: visible !important;
								margin-top: 0 !important;
							}
						}
						> .f-nav-three{
							position: absolute;
							top: 0;
							left: 100%;
							background: #fff;
							min-width: 140px;
							margin-top: 30px;
							border-radius: 0 0 3px 3px;
							box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
							white-space: nowrap;
							visibility: hidden;
							opacity: 0;
							z-index: 1;
							transition: all 0.3s ease-in-out;
