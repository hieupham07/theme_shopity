/*-------variables--------*/
$colorFullPage:{{settings.shop_text_color}}; 
$colorMain:{{settings.btn_bg_shop}}; 
$colorTitle:{{settings.shop_title_color}};
$colorHover:{{settings.shop_text_hover_color}};
$colorAddCart:{{settings.btn_bg_addcart_shop}};
$colorbgFooter:{{settings.footer_bg_color}};
$colorborderFooter:{{settings.footer_border_color}};
$colorTitleFooter:{{settings.footer_title_color}};
$colorTextFooter:{{settings.footer_text_color}};
$colorbgFooterCopyrigfht:{{settings.footer_bg_color_copyright}};
$colorborderFooterCopyright:{{settings.footer_border_color_copyright}};
$colorTextFooterCopyright:{{settings.footer_text_color_copyright}};
$colorbgFooterCopyright:{{settings.footer_bg_color_copyright}};

:root {
	--font-size-title-first-home: 20px;
	--font-size-title-second-home: 32px;
	--font-size-description-home: 15px;
	--font-size-body: 14px;
}
@media (min-width: 768px) and (max-width: 991px){
	:root {
		--font-size-title-first-home: 18px;
		--font-size-title-second-home: 24px;
		--font-size-description-home: 15px;
	}
}
@media(max-width: 767px){
	:root {
		--font-size-title-first-home: 16px;
		--font-size-title-second-home: 24px;
		--font-size-description-home: 15px;
	}
}
@mixin heading-title-first {font-size: var(--font-size-title-first-home);display: block;margin: 0 0 10px;font-weight: 400;line-height: 1.3;}
@mixin heading-title-second {font-size: var(--font-size-title-second-home);margin: 0;line-height: 1.3;font-weight: 500;text-transform: uppercase;}
@mixin description-home {font-size: var(--font-size-description-home);line-height: 1.5;font-weight: 400;margin: 0;padding-top: 15px;letter-spacing: 0.2px; text-align: center;}
/*--------------------------------------------------------------------------------
1. GENERAL
--------------------------------------------------------------------------------*/
@font-face {
	font-family: 'FontAwesome';
	src: url('{{"fontawesome.eot" | asset_url }}');
	src: url('{{"fontawesome.eot" | asset_url }}') format('embedded-opentype'), url('{{"fontawesome.woff" | asset_url }}') format('woff'), url('{{"fontawesome-webfont.ttf" | asset_url }}') format('truetype'), url('{{"fontawesome-webfont.svg" | asset_url }}') format('svg');
	font-weight: normal;
	font-style: normal;
	font-display: swap;
} 
*{margin: 0;padding: 0;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.fa{font-family: 'FontAwesome'!important;}
html,body{border: 0;background-color: #fff;}
html{-webkit-overflow-scrolling: touch;-webkit-text-size-adjust:none;-ms-text-size-adjust:100%;}
img{max-width:100%;}
{%- assign font = settings.base_font | split : ':' | first -%}
body{
	position: initial!important;
	line-height: 1.4;
	font-size: var(--font-size-body);
	text-align: initial;
	font-family: {{ font }},sans-serif;  
	color: $colorFullPage;
	overflow-x: hidden;
	background-color: #fff;
}	
/* 1.2 - HTML5 Elements */
article, aside, details, figcaption, figure, dialog, footer, header, hgroup, menu, nav, section { display:block; }
/* 1.3 - Typography */
h1,h2,h3,h4,h5,h6 { color:$colorTitle; margin:0 0 13px 0;font-weight: 500;line-height:1.2;font-family: 'Montserrat', sans-serif;}
h1 { font-size: 36px;}
h2 { font-size: 28px;}
h3 { font-size: 24px;}
h4 { font-size: 13px; line-height: 18px; }
h5 { font-size: 12px; line-height: 18px; }
h6 { font-size: 10px; line-height: 18px; text-transform:uppercase; }
h1 a, h2 a, h3 a, h4 a, h5 a, h6 a{color: $colorTitle;font-family: 'Montserrat', sans-serif;}
b, strong{font-weight: 700;}
.btn:focus{box-shadow: none;}
svg{vertical-align: initial;}
/* 1.4 - link */
a{color: $colorFullPage;text-decoration: none; outline: none;}
a:hover{text-decoration: none; outline: none;color: $colorHover;}	
a:active,a:focus { outline: none;color: $colorFullPage; }
button:focus,input:focus{outline: none;}
button.add-toCart[disabled],button[disabled]{cursor: default;opacity: .7;}
.link{color: #000;-webkit-transition: color 0.2s ease-in-out;-moz-transition: color 0.2s ease-in-out;transition: color 0.2s ease-in-out;}
/* 1.5 - List  */
ul{	padding:0;margin: 0;list-style-type:none;}
/* 1.6 - Tables */
table { width: 100%; border-spacing: 0; }
table tr td { border-top: 1px solid #dddddd; }
table tr td, table tr th { padding: 10px; text-align: left; }
table tr td:first-child, table tr th:first-child { padding-left: 0px; }
table tr td:last-child, table tr th:last-child { padding-right: 0px; }
strong { font-weight: 700; }
em { font-style: italic; }
/* 1.7 - button */
.btn-box{
	display: inline-block;
	position: relative;
	outline: 0;
	font-style: normal;
	font-weight: 500;
	padding: 15px 25px;
	font-size: 13px;
	line-height: 13px;
	text-align: center;
	text-transform: uppercase;
	letter-spacing: 1px;
	color: #fff;
	background-color: transparent;
	border: 1px solid $colorMain;
	border-radius: 0;
	-webkit-transform:  translateX(0);
	transform:  translateX(0);
	-webkit-transition: all  0.3s ease-out;
	transition: all  0.3s ease-out;
	cursor: pointer;
	overflow: hidden;
	&:before{
		content: "";
		position: absolute;
		z-index: -1;
		top: 0;
		bottom: 0;
		left: 0;
		right: 0;
		background-color: #fff;
		-webkit-transform: scaleX(0);
		transform: scaleX(0);
		-webkit-transition: all  0.3s ease-out;
		transition: all  0.3s ease-out;
	}
	&.dark {background-color: $colorMain;border-color: $colorMain;}
	&.btnred {background-color: $colorAddCart;border-color: $colorAddCart;}
	@media(min-width: 768px){
		&:hover{
			&.dark{color: $colorFullPage;}
			&.btnred{color: $colorFullPage;}
			&:before{
				-webkit-transform: scaleX(1);
				transform: scaleX(1);
			}
		}
	}
}
.sitebox-recaptcha{
	padding: 0;color: #898989;text-align: left;font-size: 13px;clear: both;
	a{color: #2962ff;}
}
.site-overlay {
	background: rgba(225, 225, 225, 0.05);position: fixed;left: 0;top: 0;z-index: 88;width: 100vw;height: 100vh;
	opacity: 0;visibility: hidden;transition: all 200ms linear;
}
.locked-scroll .site-overlay{opacity: 1;visibility: visible;}
/* lazyload img*/
img.owl-lazy:not([src]){visibility: hidden;}
.aspect-ratio img:not([src]){visibility: hidden;}
.aspect-ratio{
	position: relative;
	display: block;
	.ls-blur-up-is-loading,.lazyload:not([src]) {visibility: hidden;}
	.ls-blur-up-img{
		position: absolute;top: 0;left: 0;
		width: 100%;height: 100%;display: block;
		font-family: "blur-up: auto", "object-fit: cover";object-fit: cover;
		filter: blur(10px);opacity: 1;transition: opacity 800ms, filter 1200ms;
		&.ls-inview.ls-original-loaded {opacity: 0;	filter: blur(5px);}
	}
}
/* 1.8 - animation css3 */
@-webkit-keyframes marginLeft {0% {background-position: 20%;}100% {background-position: 90%;}}
@keyframes marginLeft {0% {background-position: 20%; }100% {background-position: 90%;} }
@-webkit-keyframes slideshowFade{0%{opacity:0;-webkit-transform:scale(0.8);transform:scale(0.8)}100%{-webkit-transform:scale(1);transform:scale(1);opacity:1}}
@keyframes slideshowFade{0%{opacity:0;-webkit-transform:scale(0.8);transform:scale(0.8)}100%{-webkit-transform:scale(1);transform:scale(1);opacity:1}}
@-webkit-keyframes scaleBannerService {to {box-shadow: 0 0 0 72px rgba(232, 76, 61, 0)}}
@keyframes scaleBannerService {to {box-shadow: 0 0 0 72px rgba(232, 76, 61, 0)}}
%text-line-clamp2{white-space: initial;overflow: hidden;-webkit-line-clamp: 2;-webkit-box-orient: vertical;display: -webkit-box;}
/* 1.9 - Form control */
.form-control{
	height: initial;
	font-size: 14px;
	color: $colorFullPage;
	background: transparent;
	border: 1px solid #ebeded;
	padding: 10px 15px;
	-webkit-appearance: none;
	&:focus{
		outline: 0;
		border-color: $colorMain;
		box-shadow: none;
	}
}
.main-body{position: relative;}
.layout-pageNot h1 span{
	display: block;
	color: #fff;
	font-size: 170px;
	font-weight: bold;
	text-shadow: 0 1px 0 #ccc, 0 2px 0 #c9c9c9, 0 3px 0 #bbb, 0 4px 0 #b9b9b9, 0 5px 0 #aaa, 0 6px 1px rgba(0, 0, 0, 0.1), 0 0 5px rgba(0, 0, 0, 0.1), 0 1px 3px rgba(0, 0, 0, 0.3), 0 3px 5px rgba(0, 0, 0, 0.2), 0 5px 10px rgba(0, 0, 0, 0.25), 0 10px 10px rgba(0, 0, 0, 0.2), 0 20px 20px rgba(0, 0, 0, 0.15);
}
@media(min-width: 1200px){
	.container-fluid{max-width: 1600px;padding: 0 35px;}
}
@media(min-width: 1400px){
	.container-fluid{padding: 0 85px;}
}
/*--------------------------------------------------------------------------------
2. HEADER
--------------------------------------------------------------------------------*/
.announcement-bar{
	padding: 10px 0;
	.top-widget-holder{
		a{
			display: inline-block;
			font-size: 12px;
			line-height: 1.5;
			font-weight: 400;
			text-decoration: none;
			&:not(:first-child){margin-left: 20px;}
			&:hover{color: $colorHover;}
			i.fa:before{
				display: inline-block;
				font-family: 'FontAwesome';
				font-style: normal;
				font-weight: normal;
				line-height: 1;
				-webkit-font-smoothing: antialiased;
				-moz-osx-font-smoothing: grayscale;
				font-size: 15px;
				margin-right: 5px;
			}
		}
	}
	.top-bar-follow{
		p{margin: 0;}
		a{
			text-transform: uppercase;
			&:not(:first-child){
				margin-left: 10px;padding-left: 10px;border-left: 1px solid #d4d4d4;
			}
		}
	}
	@media(max-width: 991px){
		&{display: none;}
	}
}
.flexContainer-header{
	>div[class*="wrapper-header-"]{padding: 0 10px;}
	@media(max-width: 991px){
		>div.wrapper-header-icon{padding-left: 0;}
		>div.wrapper-header-logo{padding: 0 5px;}
		>div[class*="wrapper-header-"]{position: initial;width: initial;}
	}
}
.main-header{
	border-bottom: 1px solid #ededed;
	.navigation-header{
		position: relative;
		background: #fff;
		z-index: 99;
		width: 100%;
		&.hSticky{
			opacity: 0;
			visibility: hidden;
			&.hSticky-down{
				position: fixed;
				top: 0;
				left: 0;
				right: 0;
				z-index: 999;
				background: #fff;
				border-bottom: 0;
				-webkit-box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
				-moz-box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
				box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
				-webkit-transform: translateY(-100%);
				transform: translateY(-100%);
				-webkit-transition: all 400ms ease;
				-moz-transition: all 400ms ease;
				transition: all 400ms ease;
				&.hSticky-up{
					opacity: 1;
					visibility: visible;
					-webkit-transform: translateY(0);
					transform: translateY(0);
					-webkit-transition: all 400ms ease;
					-moz-transition: all 400ms ease;
					transition: all 400ms ease;
				}
				.announcement-bar{display:none;}
				.main-nav{text-align: center;}
			}
		}
		&:not(.hSticky-down){
			@media(min-width: 992px){
				.header_dropdown_content{top: calc(100% + 17px);}
			}
			@media(min-width: 992px) and (max-width: 1199px){
				.wrap-logo{
					h1,a{font-size: 26px;}
				}
			}
		}
		&.hSticky-down{
			@media(min-width: 992px){
				#main-nav-linklist .nav-linklist>.nav-bar-item{padding: 20px 0;}
				.wrap-logo{
					h1,a{font-size: 26px;}
				}
			}
			@media(max-width: 991px){
				.search-bar-mobile{
					position: absolute;
					right: 0;
					left: 0;
					top: 50%;
					border-top: none;
					-webkit-transform: translateY(-50%);
					transform: translateY(-50%);
					visibility: hidden;
					padding: 0px 90px 0px 50px;
					.search-box{visibility: visible;}
					.smart-search-wrapper{top: calc(100% + 8px);}
				}
			}
		}
	}
	&:not(.mainHeader_temp_2){
		.navigation-header{
			&.hSticky-down{
				.wrapper-header-logo{display: none;}
				@media(min-width: 992px){
					.header-inner{
						position: absolute;
						right: 0;
						left: 0;
						top: 50%;
						padding: 0;
						-webkit-transform: translateY(-50%);
						transform: translateY(-50%);
						visibility: hidden;
					}
					.header-nav-sticky{
						display: -webkit-flex;
						display: flex;
						-webki-align-items: center;
						align-items: center;
					}
					.wrap-logo-sticky{display: block;max-width: 18%;padding: 5px 0;}
					.menu-desktop{
						-webkit-flex: 1 1 auto;
						flex: 1 1 auto;
						padding-left: 15px;
						padding-right: 130px;
					}
					.wrapper-header-icon{visibility: visible;width: initial;max-width: initial;flex: unset;}
				}
			}
		}
	}
	&.mainHeader_tempPr{
		.wrapper-header-nav{
			.backto-page.icon-nav{display: none;}
		}
	}
	.wrap-logo{
		h1,a{
			font-size: 32px;
			font-weight: bold;
			color: $colorMain;
			margin: 0;
		}
		a img{max-height: 65px;max-width: 160px;}
	}
	.wrap-logo-sticky{display: none;}
	@media(min-width: 992px){
		.navigation-header{
			.wrap-logo-sticky,.wrap-logo{min-width: 180px;}
			&.hSticky-down{
				.wrap-logo img{max-height: 55px;}
			}
		}
		.header-inner{padding: 20px 0 15px;}
		.wrapper-header-icon{position: relative;}
	}
	@media(min-width: 992px) and (max-width: 1199px){
		#main-nav-linklist .nav-linklist>.nav-bar-item{margin: 0 10px;}
	}
	@media(max-width: 991px){
		.header-inner{
			padding: 10px 0;position: relative;
		}
		&.mainHeader_tempPr{
			.wrapper-header-nav{
				.icon-nav{display: inline-block;vertical-align: middle;}
			}
			.navigation-header{
				&.hSticky-down{
					.backto-page{
						display: inline-block;
						.backto-page-history{
							display: block;width: 30px;height: 35px;line-height: 35px;text-align: center;margin-right: 5px;
							svg{width: 30px;height: 35px;fill: #000;}
						}
					}
					.header-action-icon .wrap-search{display: block; }
					.search-bar-mobile{display: none!important;}
					.header_dropdown_content.site_menu_mobile .box-triangle{left: 48px;}
				}
			}
		}
	}
	@media(max-width: 767px){
		.wrap-logo{
			h1,a{
				font-size: 26px;
			}
			a img{max-height: 50px;max-width: 180px;}
		}
	}
}
.locked-scroll {
	@media(max-width: 991px){
		&{overflow: hidden;}
		.main-header .navigation-header,.main-header .navigation-header.hSticky-down{
			position: fixed;
			top: 0;
			left: 0;
			right: 0;
			visibility: visible;
			opacity: 1;
			-webkit-transform: translateY(0);
			transform: translateY(0);
		}
	}
}
.header-action-icon{
	.wrap-cart{
		.count-holder{
			position: absolute;
			top: 0;
			right: -2px;
			font-size: 11px;
			text-align: center;
			min-width: 18px;
			line-height: 18px;
			color: #fff;
			border-radius: 50%;
			background: $colorMain;
			-webkit-transition: opacity 0.3s ease-in-out, -webkit-transform 0.3s ease-in-out;
			-moz-transition: opacity 0.3s ease-in-out, -moz-transform 0.3s ease-in-out;
			transition: opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
			@media(max-width: 991px){
				&{
					top: 0;right: 0;min-width: 16px;line-height: 16px;
				}
			}
		}
	}
}
.header-action{
	margin-left: 10px;
	a.header-action-toggle{
		display: block;
		position: relative;
		font-size: 13px;
		color: #333;
	}
	.box-action-icon{
		display: inline-block;
		vertical-align: middle;
		position: relative;
		width: 30px;
		height: 35px;
		text-align: center;
		svg{
			width: 22px;
			height: 35px;
			fill: #000;
			-webkit-transition: opacity 0.3s ease-in-out, -webkit-transform 0.3s ease-in-out;
			-moz-transition: opacity 0.3s ease-in-out, -moz-transform 0.3s ease-in-out;
			transition: opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
		}
	}
	&.show-action{
		.header_dropdown_content{
			opacity: 1;
			visibility: visible;
			-webkit-transform: scale(1);
			transform: scale(1);
			transition: opacity 0.4s cubic-bezier(0, 1, 0.4, 1), visibility 0.4s linear, -webkit-transform 0.4s cubic-bezier(0.18, 1.25, 0.4, 1);
			transition: opacity 0.4s cubic-bezier(0, 1, 0.4, 1), transform 0.4s cubic-bezier(0.18, 1.25, 0.4, 1), visibility 0.4s linear;
			transition: opacity 0.4s cubic-bezier(0, 1, 0.4, 1), transform 0.4s cubic-bezier(0.18, 1.25, 0.4, 1), visibility 0.4s linear, -webkit-transform 0.4s cubic-bezier(0.18, 1.25, 0.4, 1);
		}
		#account-popover{
			.popover-panel_list .popover-panel.is-selected{visibility: visible;}
		}
		@media(max-width: 991px){
			.box-action-icon{
				.svg--icon, .count-holder{opacity: 0;transform: scale(0);}
			}
			.icon-state .box-icon--close{
				opacity: 1;
				-webkit-transform: scale(1);
				-moz-transform: scale(1);
				transform: scale(1);
			}
		}
	}
	@media(max-width: 991px){
		&.wrap-search{display: none;}
	}
}
.icon-state{
	.box-icon--close{
		position: absolute;
		top: 0;
		left: 0;
		bottom: 0;
		right: 0;
		opacity: 0; 
		transform: scale(0);
		-webkit-transition: opacity 0.3s ease-in-out, -webkit-transform 0.3s ease-in-out;
		-moz-transition: opacity 0.3s ease-in-out, -moz-transform 0.3s ease-in-out;
		transition: opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
		svg{width: 20px;height: 35px;color: #000;}
	}
}
.search-bar-mobile{
	padding: 5px;
	form.searchform{
		input.searchinput{
			height: 32px;
			padding: 3px 40px 3px 8px;
			border-radius: 0;
			background: #f3f5f6;
			font-size: 12px;
		}
		.btn-search{
			width: 35px;
			height: 32px;
			line-height: 32px;
			background: transparent;
			border-radius: 0;
			svg{
				width: 14px;
				height: 32px;
				opacity: .5;
				fill: initial;
			}
			span{display: block;width: 35px;height: 32px;}
			span.search-close{
				display: none;
				svg{
					width: 20px;
					fill: $colorMain;
					opacity: initial;
				}
			}
		}
		&.expanded{
			.btn-search{
				span.search-icon{display: none;}
				span.search-close{display: block;}
			}
		}
	} 
	.smart-search-wrapper{
		position: absolute;
		top: 100%;
		left: 0;
		right: 0;
		background: #fff;
		z-index: 99;
		box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.08);
	}
}
/*------------------- Header-template-01 ---------------------*/
.announcement_bar_temp01{
	background: {{settings.header_01_top_bar_bg}};border-bottom: 1px solid {{settings.header_01_top_bar_bg}};
	a{color: {{settings.header_01_top_bar_text}};}
}
.mainHeader_temp_1{
	.navigation-header{
		.main-nav{text-align: center;}
		&:not(.hSticky-down){
			.header-inner{background: {{settings.header_01_color_bkg}};}
			.header-action-icon .header-action .box-action-icon svg{fill: {{settings.header_01_color_text}};color: {{settings.header_01_color_text}};}
			.wrap-logo h1,.wrap-logo a{color: {{settings.header_01_color_text}};}
			.hamburger-menu svg{color: {{settings.header_01_color_text}};}
		}
	}
}
/*------------------- Header-template-02 ---------------------*/
.announcement_bar_temp02{
	background: {{settings.header_02_top_bar_bg}};border-bottom: 1px solid {{settings.header_02_top_bar_bg}};
	a{color: {{settings.header_02_top_bar_text}};}
}
.mainHeader_temp_2{
	.navigation-header{
		&:not(.hSticky-down){
			.header-inner{background: {{settings.header_02_color_bkg}};}
			.header-action-icon .header-action .box-action-icon svg{fill: {{settings.header_02_color_text}};color: {{settings.header_02_color_text}};}
			.wrap-logo h1,.wrap-logo a{color: {{settings.header_02_color_text}};}
			.hamburger-menu svg{color: {{settings.header_02_color_text}};}
			.wrapper-header-menu{-webkit-flex: 1 1 auto;flex: 1 1 auto;}
			.f-nav {
				.f-nav-one > li{
					padding: 0 0;
					> a{
						color: {{settings.header_02_color_text}};
						&:hover{color: {{settings.header_02_color_hover_text}};}
